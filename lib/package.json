{
  "name": "geteventstore-promise",
  "version": "1.3.0",
  "description": "GetEventStore client wrapper using promises",
  "main": "index.js",
  "scripts": {
	"test": "node node_modules/cross-env/dist/bin/cross-env NODE_ENV=test node node_modules/mocha/bin/mocha --exit --require babel-core/register tests/",
	"build": "node_modules/.bin/babel --ignore node_modules,tests -D -d dist/ ./",
    "clean": "rm -r -f dist/",
    "includeSource": "rsync -av --exclude='dist/' --exclude='node_modules' ./ dist/src",
    "compile": "npm run clean && npm run build && npm run includeSource"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RemoteMetering/geteventstore-promise.git"
  },
  "keywords": [
    "node",
    "client",
    "event store",
    "geteventstore",
    "eventstore",
    "promise"
  ],
  "author": "Remote Metering Solutions Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RemoteMetering/geteventstore-promise/issues"
  },
  "homepage": "https://github.com/RemoteMetering/geteventstore-promise",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.1",
    "debug": "^3.1.0",
    "event-store-client": "^0.0.11",
    "lodash": "^4.17.4",
    "node-eventstore-client": "^0.1.9",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.0",
    "mocha": "^4.0.1"
  }
}
