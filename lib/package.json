{
  "name": "geteventstore-promise",
  "version": "3.0.0",
  "description": "GetEventStore client wrapper using promises",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --exit --require babel-core/register tests/",
    "test:docker": "cross-env NODE_ENV=test TEST_USE_DOCKER=true mocha --exit --require babel-core/register tests/",
    "transpile": "babel --ignore node_modules,tests,yarn.lock -D -d dist/ ./",
    "clean": "rm -r -f dist/",
    "includeSource": "rsync -av --exclude='dist/' --exclude='node_modules' ./ dist/src && cp -rf ../README.md dist/",
    "build": "npm run clean && npm run transpile && npm run includeSource"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RemoteMetering/geteventstore-promise.git"
  },
  "keywords": [
    "node",
    "client",
    "event store",
    "geteventstore",
    "eventstore",
    "promise"
  ],
  "author": "Remote Metering Solutions Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RemoteMetering/geteventstore-promise/issues"
  },
  "homepage": "https://github.com/RemoteMetering/geteventstore-promise",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0",
    "debug": "^4.0.1",
    "generic-pool": "^3.4.2",
    "node-eventstore-client": "^0.2.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0"
  }
}
