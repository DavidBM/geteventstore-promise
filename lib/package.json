{
  "name": "geteventstore-promise",
  "version": "1.2.5",
  "description": "GetEventStore client wrapper using promises",
  "main": "index.js",
  "scripts": {
    "test": "node node_modules/mocha/bin/mocha --require babel-core/register tests/",
    "clean": "rm -r -f dist/",
    "includeSource": "rsync -av --exclude='dist/' --exclude='node_modules' ./ dist/src",
    "compile": "npm run clean && babel --ignore node_modules,tests -D -d dist/ ./ && npm run includeSource"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RemoteMetering/geteventstore-promise.git"
  },
  "keywords": [
    "node",
    "client",
    "event store",
    "geteventstore",
    "eventstore",
    "promise"
  ],
  "author": "Remote Metering Solutions Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RemoteMetering/geteventstore-promise/issues"
  },
  "homepage": "https://github.com/RemoteMetering/geteventstore-promise",
  "dependencies": {
    "bluebird": "^3.5.0",
    "debug": "^2.6.3",
    "event-store-client": "^0.0.11",
    "eventstore-node": "^0.0.30",
    "lodash": "^4.17.4",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "mocha": "^3.2.0"
  }
}